<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.meurepertorio.meurepertorio">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_mr"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_mr"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MusicasActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.MusicaActivity"
            android:parentActivityName=".activity.MusicasActivity"
            android:screenOrientation="portrait"/>

    </application>

</manifest>